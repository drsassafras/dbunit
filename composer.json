{
    "name": "drsassafras/dbunit",
    "description": "PHPUnit extension for database interaction testing",
    "type": "library",
    "keywords": [
        "database",
        "testing",
        "xunit"
    ],
    "homepage": "https://github.com/drsassafras/dbunit/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dr. Sassafras",
            "email": "drsassafras@gmail.com",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/drsassafras/dbunit/issues"
    },
    "require": {
        "php": "^7.2",
        "phpunit/phpunit": "^8.0",
        "symfony/yaml": "^4.0 || ^5.0",
        "ext-pdo": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}
